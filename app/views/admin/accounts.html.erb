<h2>Accounts</h2>
<table class="table table-striped">
    <thead>
        <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Date Created</th>
        <th>Profile</th>
        <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @accounts.each do |account| %>
            <tr>
                <td><%= account.full_name %></td>
                <td><%= account.email %></td>
                <td><%= account.created_at.strftime("%d/%m/%Y") %></td>
                <td><%= link_to "View Profile", profile_path(account.id) %></td>
            </tr>
        <% end %>
    </tbody> 
</table> 
